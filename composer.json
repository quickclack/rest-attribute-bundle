{
  "name": "quickclack/rest-attribute-bundle",
  "description": "Modern REST support for Symfony 6.4â€“7.2 using attributes",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["symfony", "rest", "api", "attributes", "symfony-bundle"],
  "authors": [
    {
      "name": "quickclack",
      "homepage": "https://github.com/quickclack"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/validator": "^6.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/browser-kit": "^6.4 || ^7.0",
    "symfony/http-client": "^6.4 || ^7.0",
    "symfony/phpunit-bridge": "^6.4 || ^7.0",
    "symfony/yaml": "^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Quickclack\\RestAttributeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Quickclack\\RestAttributeBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
